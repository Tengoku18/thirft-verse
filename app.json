{
  "expo": {
    "name": "ThriftVerse",
    "slug": "thrift-verse",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/logo/ios-app-icon.png",
    "scheme": "thriftverse",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.thriftverse.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.thriftverse.app",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/android-icon1.png",
        "backgroundImage": "./assets/logo/android-base1.png",
        "backgroundColor": "#FFFFFF"
      },
      "edgeToEdgeEnabled": true,
      "softwareKeyboardLayoutMode": "resize",
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/logo/fav-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/logo/splash-screen.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#C08B7B"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "owner": "thriftme",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6c1851d4-a94c-42f6-942c-c2d2f223aaa8"
      }
    }
  }
}
